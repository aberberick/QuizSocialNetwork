package user;

public enum Achievement {

	
	AMATEUR_AUTHOR("Amateur Author","You've created a quiz!"),
	PRODIGIOUS_AUTHOR("Prodigious Author", "You've created 10 quizzes!"),
	QUIZ_MACHINE("Quiz Machine","You've taken 10 quizzes!"),
	I_AM_THE_GREATEST("I Am The Greatest", "You had the highest score on a quiz!"),
	PRACTICE_MAKES_PERFECT("Practice Makes Perfect", "You took a quiz in practice mode"),
	PROLIFIC_AUTHOR("Prolific Author", "You created five quizes!"),
	SOCIAL_BUTTERFLY("Social Butterfly", "You have 5 friends!");
	
	
	
	public String title;
	public String description;
	
	
	private Achievement(String title, String description){
		this.title = title;
		this.description = description;
	}
	
	
	
	
}
